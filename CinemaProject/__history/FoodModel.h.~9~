//---------------------------------------------------------------------------

#ifndef FoodModelH
#define FoodModelH

#include "JsonObject.h"

class FoodModel : public TJsonObject
{
   String name;
   String price;

 public:
   FoodModel();

   __property String    Name   = {read = name, write = name };

 protected:
 private:
   void         FromJson(TJSONObject *aObject);
   TJSONObject *ToJson();
};
//---------------------------------------------------------------------------
#endif