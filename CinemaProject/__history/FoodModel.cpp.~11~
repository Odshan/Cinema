FoodModel::FoodModel()
{
}

void FoodModel::FromJson(TJSONObject *aObject)
{
   d_name = GetString(aObject, "name");
   d_price = GetString(aObject, "price");
}

TJSONObject *MovieObject::ToJson()
{
   auto o = new TJSONObject();

   AddPair(o, "xxx", xxx);

   return o;
}