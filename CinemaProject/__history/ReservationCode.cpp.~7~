//---------------------------------------------------------------------------

#include <vcl.h>
#pragma hdrstop

#include "ReservationCode.h"

#include "MyReservation"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
TForm4 *Form4;
//---------------------------------------------------------------------------
__fastcall TForm4::TForm4(TComponent* Owner)
	: TForm(Owner)
{
}
//---------------------------------------------------------------------------

void __fastcall TForm4::Label1Click(TObject *Sender)
{
	if (Edit1->Text != "") {
	std::string * o = reinterpret_cast<std::string *>(Edit1->Text);
	Hide();
	Form5->SetReservationCode(o);
	Form5->ShowModal();
	Close();
	}
}

//---------------------------------------------------------------------------

